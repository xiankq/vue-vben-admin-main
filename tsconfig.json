{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@vben/tsconfig/web-app.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#/*": ["./src/*"]
    },
    "types": [
      "unplugin-vue-router/client"
    ]
  },
  "references": [{ "path": "./tsconfig.node.json" }],
  "include": [
    "src/**/*.ts",
    "typings/*",
    "src/**/*.tsx",
    "src/**/*.vue",
    "apps/**/*"
  ]
}
